    -fx-font-family: "Arial";
    -fx-text-fill: linear-gradient(white, #d0d0d0);
    -fx-font-size: 12px;
    -fx-padding: 10 10 10 10 ; 
	-fx-max-width: 1122px;
	-fx-min-width: 1122px;
	}

#login_button{
	    -fx-background-color: 
        #090a0c,
        linear-gradient(#38424b 0%, #1f2429 20%, #191d22 100%),
        linear-gradient(#20262b, #191d22),
        radial-gradient(center 50% 0%, radius 100%, SteelBlue, rgba(255,255,255,0));
    -fx-background-radius: 5,4,3,5;
    -fx-text-fill: white;
    -fx-effect: dropshadow( three-pass-box , rgba(0,0,0,0.6) , 5, 0.0 , 0 , 1 );
    -fx-font-family: "Arial";
    -fx-text-fill: linear-gradient(white, #d0d0d0);
    -fx-font-size: 16px;
    -fx-padding: 10 10 10 10 ; 
}
#login_button:pressed, #login_button:default:hover:pressed
{
  -fx-background-color: black;
  -fx-text-fill: white;
}
 
/*******************************************************************************
 *                                                                             *
 * KASSA Buttons                                                                 *
 *                                                                             *
 ******************************************************************************/
#article_button {

	-fx-max-width: 96px;
	-fx-max-height: 140px;
        -fx-padding: 0 0 0 0;
}
#article_button:pressed, #article_button:default:hover:pressed
{
  -fx-background-color: black;
  -fx-text-fill: white;
  -fx-border-width: 0;
}
#article_button:focused
{
