  -fx-background-color: transparent;
  -fx-text-fill: white;
  -fx-border-width: 0.3;
  -fx-border-color: DimGray;
}
/* Markerad rad när tabellen är i fokus */
.table-view:focused {
  -fx-background-color: transparent;
  -fx-text-fill: white;
  -fx-border-width: 0.3;
  -fx-border-color: DimGray;
}
/* Markerad rad när tabellen inte är i fokus */
.table-row-cell:filled:focused:selected {
    -fx-background-color: dodgerblue;
    -fx-background-insets: 0, 0, 0;
    -fx-background: -fx-accent;
    -fx-text-fill: -fx-selection-bar-text;
	-fx-border-width: 0;
